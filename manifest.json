{
    "name": "Shortcuts for LexisAdvance Website",
	"description": "Adds keyboard shortcuts for LexisAdvance(R) website.",
	"version": "1.0.1",
	"author": "Owen Healy <owenehealy@gmail.com>",
    "manifest_version": 2,
    "permissions": [
		"storage",
        "https://advance.lexis.com/*",
        "http://advance.lexis.com/*"
    ],
    "content_scripts": [
		{
			"matches": ["https://advance.lexis.com/*", "http://advance.lexis.com/*"],
			"js": ["third-party/jquery-3.1.1.min.js", "contentscript.js"]
		}
	],
	"web_accessible_resources": [
		"no-highlight.css"
	],
	"options_page": "options.html",
	"icons": {
		 "16": "icon-16.png",
		 "48": "icon-48.png",
		"128": "icon-128.png"
	}
}
